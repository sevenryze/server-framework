{
  "name": "@sevenryze/server",
  "private": false,
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --collectCoverage --mapCoverage",
    "dev": "tsc --watch --project .",
    "build": "tsc --project .",
    "commit": "npm build && npm test && npm publish --access=public"
  },
  "dependencies": {
    "@types/node": "^9.6.1",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "request-ip": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "superagent": "^3.8.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "description": "Backend Server Framework of DeepDigEst Inc.",
  "keywords": ["DeepDigEst Inc.", "Server", "Kerberos", "Framework"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "homepage": "https://www.deepdigest.com",
  "bugs": {
    "url": "https://github.com/sevenryze",
    "email": "sevenryze@gmail.com"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "sevenryze",
    "email": "sevenryze@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sevenryze"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "testMatch": ["<rootDir>/test/**/*.ts?(x)"]
  }
